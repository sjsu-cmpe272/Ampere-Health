                        <a href="#" class="dropdown-toggle" data-toggle="dropdown"><i
                           class="icon-user"></i> <%= user.fitbit.name %><b class="caret"></b></a>
                        <ul class="dropdown-menu">
                           <li><a href="/profile">Profile</a></li>
                           <li><a href="/logout">Logout</a></li>
                        </ul>
                     </li>
                  </ul>
                  <!--<form class="navbar-search pull-right">
                     <input type="text" class="search-query" placeholder="Search">
                  </form>-->
               </div>
               <!--/.nav-collapse -->
            </div>
            <!-- /container -->
         </div>
         <!-- /navbar-inner -->
      </div>
      <!-- /navbar -->


      <div class="subnavbar">

      	<div class="subnavbar-inner">

      		<div class="container">

      			<ul class="mainnav">
                        <li ><a href="/dashboard"><i class="icon-dashboard"></i><span>Dashboard</span> </a> </li>
                        <li><a href="/report"><i class="icon-list-alt"></i><span>Analysis</span> </a> </li>
                        <li><a href="/restaurant"><i class="icon-food"></i><span>Restaurants</span> </a> </li>
                        <li class="active"><a href="/profile"><i class="icon-user"></i><span>Profile</span> </a> </li>


              </ul>
      				</li>

      			</ul>

      		</div> <!-- /container -->

      	</div> <!-- /subnavbar-inner -->

      </div> <!-- /subnavbar -->
      <!-- /subnavbar -->
      <div class="main">
         <div class="main-inner">
            <div class="container">
               <div class="row">
                  <div class="span12">
                     <div class="widget ">
                        <div class="widget-header">
                           <i class="icon-user"></i>
                           <h3>Your Account</h3>
                        </div>
                        <br>
                        <!-- /widget-header -->
                        <div class="widget-content">
                           <div class="tabbable">
                              <ul class="nav nav-tabs">
                                 <li class="active">
                                    <a href="#formcontrols" data-toggle="tab">My Profile</a>
                                 </li>
                                 <li><a href="#jscontrols" data-toggle="tab">Challenges</a></li>
                              </ul>
                              <br>
                              <div class="tab-content">
                                 <div class="tab-pane active" id="formcontrols">
                                    <form id="edit-profile active" class="form-horizontal">
                                       <fieldset>
                                          <table class='table table-user-information'>
                                                  <tbody>
                                                    <tr>
                                                      <td>Name:</td>
                                                      <td><%= user.fitbit.name %></td>
                                                    </tr>
                                                    <tr>
                                                      <td>Age:</td>
                                                      <td><%= user.fitbit.age %></td>
                                                    </tr>
                                                    <tr>
                                                      <td>Gender</td>
                                                      <td><%= user.fitbit.gender %></td>
                                                    </tr>

                                                       <tr>
                                                           <tr>
                                                      <td>Height</td>
                                                      <td><%= user.fitbit.height %></td>
                                                    </tr>
                                                      <tr>
                                                      <td>Weight</td>
                                                      <td><%= user.fitbit.weight %></td>
                                                    </tr>
                                                    <tr>
                                                      <td>Date of Birth</td>
                                                      <td><%= user.fitbit.dateofbirth %></td>
                                                    </tr>
                                                  </tbody>
                                                </table>
                                       </fieldset>
                                    </form>
                                 </div>
                                 <div class="tab-pane" id="jscontrols">
                                    <form id="edit-profile2" class="form-vertical">
                                       <fieldset>
                                          <div class="control-group">

                                             <div class="controls">


                                               <table class="table table-striped table-bordered" id="friend123">
                                                   <tr>
                                    					        <th>Name</th>

                                    					        <th>Average Daily Steps</th>
                                                      <th></th>
                                    					    </tr>

                                                </table>

<!--
                                             </div>
                                             <!-- /controls -->
                                          </div>
                                          <!-- /control-group -->

                                       </fieldset>
                                    </form>
                                 </div>
                              </div>
                           </div>
                        </div>
                        <!-- /widget-content -->
                     </div>
                     <!-- /widget -->
                  </div>
                  <!-- /span8 -->
               </div>
               <!-- /row -->
            </div>
            <!-- /container -->
         </div>
         <!-- /main-inner -->
      </div>
      <!-- /main -->
      <div class="footer">
         <div class="footer-inner">
            <div class="container">
               <div class="row">
                  <div class="span12">
                     &copy; 2016 <a href="#">Ampere</a>.
                  </div>
                  <!-- /span12 -->
               </div>
               <!-- /row -->
            </div>
            <!-- /container -->
         </div>
